.user-info {
  height: 348rpx;
  color: #fff;
  position: relative;
}

.user-info .user-info-bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}

.user-info .avatar {
  width: 130rpx;
  height: 130rpx;
  border-radius: 99999rpx;
  border: 5rpx solid #fff;
  margin: 0 24rpx;
}

.user-info .user-name {
  font-weight: bold;
}

.user-info .my-address {
  background: #ff4544;
  color: #fff;
  border-radius: 999rpx 0 0 999rpx;
  padding-right: 24rpx;
}

.user-info .my-address image {
  width: 44rpx;
  height: 44rpx;
  margin: 16rpx;
}

.order-block {
  /* background: #fff; */
  margin-bottom: 20rpx;
}

.order-block .my-order {
  display: flex;
  justify-content: space-between;
  align-items: center;
  /* border-bottom: 1rpx solid #e3e3e3; */
  padding: 24rpx;
}

.order-block .my-order .right_icon {
  height: 22rpx;
  width: 12rpx;
}

.order-block .nav-item {
  text-align: center;
  display: block;
  font-size: 9pt;
  padding: 24rpx 0;
  position: relative;
}

.order-block .nav-item .num-icon {
  position: absolute;
  top: 8rpx;
  left: 50%;
  margin-left: 12rpx;
  display: inline-block;
  background: #f00;
  color: #fff;
  border-radius: 999rpx;
  padding: 0 8rpx;
  min-width: 40rpx;
  font-size: 7pt;
}

.order-block .nav-item image {
  width: 60rpx;
  height: 57rpx;
  margin-bottom: 5rpx;
}

.option-list {
  margin-bottom: 20rpx;
}

.option-item {
  background: #fff;
  padding: 0 24rpx;
  display: block;
  position: relative;
}

.option-item .option-content {
  border-bottom: 1rpx solid #eee;
  height: 110rpx;
}

.option-item .option-icon {
  width: 50rpx;
  height: 50rpx;
  margin-right: 24rpx;
}

.option-item .option-jisntou {
  width: 12rpx;
  height: 22rpx;
}

button.option-item {
  font-size: inherit;
  color: inherit;
  font-family: inherit;
  border: none;
  background: #fff;
}

.user-level {
  margin-top: 16rpx;
  position: relative;
}

.user-level image {
  width: 44rpx;
  height: 44rpx;
  margin-right: 10rpx;
  margin-left: -4rpx;
}

.level-name {
  height: 36rpx;
  border-radius: 18rpx;
  font-size: 8pt;
  color: #fff;
  background-color: #3c3642;
  padding: 0 16rpx 0 0;
}

.integral-bg, .integral {
  height: 100rpx;
  width: 100%;
  position: absolute;
  bottom: 0;
  left: 0;
}

.integral-bg {
  background-color: #888;
  opacity: 0.1;
}

.integral {
  padding: 0 27rpx;
  color: #353535;
  font-size: 12pt;
}

.integral text {
  margin-left: 20rpx;
}

.integral image {
  width: 52rpx;
  height: 54rpx;
  margin-right: 20rpx;
}

.userinfo-name, .userinfo-addr {
  height: 248rpx;
}

.integral-num {
  margin-left: 20rpx;
}

.user-integral-arrow image {
  width: 16rpx;
  height: 26rpx;
}

.user-integral-go {
  font-size: 11pt;
  color: #666;
  margin-right: 20rpx;
}

.shadow-btn {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  padding: 0;
  margin: 0;
  background-color: rgba(0, 0, 0, 0);
  border: 0;
  border-radius: 0;
}

button::after {
  transform: scale(0);
}

.num-1 {
  color: #ffbb43;
  font-size: 13pt;
  margin-bottom: 16rpx;
}

.num-2 {
  color: #666;
  align-items: center;
  line-height: 1;
}

.wallet {
  width: 100%;
  height: 140rpx;
  margin-bottom: 20rpx;
}

.wallet-1 {
  background-color: #fbfbfb;
  width: 310rpx;
  height: 100%;
}

.wallet-2 {
  background-color: #fff;
  width: 440rpx;
  height: 100%;
}

.wallet-3 {
  background-color: #fff;
  width: 300rpx;
  height: 100%;
}

.wallet .wallet-3:last-child .flex-grow-1 {
  border-left: 1rpx solid #e2e2e2;
}

.option-item.style {
  width: 25%;
  padding: 24rpx;
  font-size: 9pt;
}

.option-item.style .option-icon {
  margin: 0;
  margin-bottom: 12rpx;
}

.user-info-1 {
  margin-top: 50rpx;
  background-color: #fff;
  border-radius: 10rpx;
  height: 200rpx;
  width: 700rpx;
  color: #666;
  box-shadow: 0px 0px 12rpx 1rpx #ddd;
}

.home-content {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.home-bg {
  width: 100%;
  height: 690rpx;
}

/*图标和姓名*/

.home-content-top {
  width: 100%;
  height: 200rpx;
}

.content-top-img {
  width: 127rpx;
  height: 127rpx;
  text-align: center;
  margin: 30rpx auto 28rpx;
  border-radius: 50%;
  display: block;
}

.content-top-name {
  font-size: 40rpx;
  color: #fff;
  text-align: center;
}

/*收益与余额*/

.home-content-money {
  margin: 0 26rpx;
  overflow: hidden;
}

.content-money-left, .content-money-right {
  width: 50%;
  height: 148rpx;
  text-align: center;
  color: #fff;
}

.content-money-left {
  float: left;
}

.content-money-right {
  float: right;
}

.money-left-sy, .money-right-sy {
  font-size: 34rpx;
  margin: 30rpx auto 22rpx;
  font-weight: 600;
}

.money-left-title, .money-right-title {
  font-size: 26rpx;
}

/*个人信息*/

.home-content-information {
  position: absolute;
  width: 185rpx;
  height: 60rpx;
  background: #d61d1d;
  top: 83rpx;
  right: 0;
  overflow: hidden;
  border-top-left-radius: 30rpx;
  border-bottom-left-radius: 30rpx;
  line-height: 60rpx;
}

.content-information-icon {
  width: 27rpx;
  height: 27rpx;
  margin-top: 17rpx;
  margin-left: 22rpx;
  margin-right: 11rpx;
  display: block;
  float: left;
}

.content-information-title {
  font-size: 24rpx;
  color: #fff;
}

/* 我的订单 */

.content-order-title {
  margin-top: 10rpx;
  margin-left: 30rpx;
  font-size: 0.9em;
  margin-bottom: 70rpx;
}

.content-order-con {
  width: 100%;
  height: 150rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.content-order-con navigator {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 0.75em;
}

.order_img {
  width: 50rpx;
  height: 50rpx;
  margin-bottom: 10rpx;
}

/* 分割线 */

.blank_br {
  width: 100%;
  height: 30rpx;
  background: #fafafa;
}

/* 卡券数量 */

.coupon_num {
  width: 50rpx;
  height: 50rpx;
  background-color: #f00;
  text-align: center;
  line-height: 50rpx;
  color: #fff;
  border-radius: 50%;
  margin-right: 10rpx;
}
